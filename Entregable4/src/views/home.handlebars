<div class="content">    
    <h1>Productos en Venta</h1>
    <div class="cards-products"> 
        {{#each products}}
        <div class="card-product">
            <div class="card-product__img"> 
                <img src="{{this.thumbnail}}" alt="{{this.title}}" class="card-product__img_file">
            </div>
            <div>
                <p class="card-product__title">{{this.title}} </p>
                <p class="card-product__price">COP$ {{this.price}} </p>
            </div>
            <div class="card-product__btn">
                <button class="detailsButton" id={{this.id}}><ion-icon name="search-circle-outline"></ion-icon> Detalles</button>
            </div>
        </div>
        {{/each}}
    </div>
</div><script src="/js/products.js"></script>